{
  "general": {
    "id": "test_highway",
    "seed": 0,
    "workspace": "tmp/test_highway",
    "dictConfig": {
      "version": 1,
      "disable_existing_loggers": false,
      "formatters": {
        "standard": {
          "format": "[%(levelname)s] %(message)s"
        }
      },
      "handlers": {
        "default": {
          "level": "INFO",
          "formatter": "standard",
          "class": "logging.StreamHandler"
        },
        "file": {
          "level": "INFO",
          "formatter": "standard",
          "filename": "test_highway.log",
          "class": "logging.FileHandler"
        }
      },
      "loggers": {
        "": {
          "handlers": [
            "default", "file"
          ],
          "level": "WARN",
          "propagate": false
        },
        "ncarrara.bftq_pydial.tools.features": {
          "handlers": [
            "default", "file"
          ],
          "level": "INFO",
          "propagate": false
        },
        "ncarrara.utils_rl.algorithms.dqn": {
          "handlers": [
            "default", "file"
          ],
          "level": "INFO",
          "propagate": false
        },
        "ncarrara.bftq_pydial.bftq.pytorch_budgeted_fittedq": {
          "handlers": [
            "default", "file"
          ],
          "level": "INFO",
          "propagate": false
        },
        "ncarrara.utils_rl.algorithms.pytorch_fittedq": {
          "handlers": [
            "default", "file"
          ],
          "level": "INFO",
          "propagate": false
        },
        "ncarrara.utils_rl.environments.slot_filling_env.slot_filling_env": {
          "handlers": [
            "default", "file"
          ],
          "level": "WARN",
          "propagate": false
        },
        "ncarrara.utils.math": {
          "handlers": [
            "default", "file"
          ],
          "level": "INFO",
          "propagate": false
        },
        "ncarrara.utils.os": {
          "handlers": [
            "default", "file"
          ],
          "level": "INFO",
          "propagate": false
        }
      }
    }
  },
  "feature_str": "feature_identity",
  "generate_envs": {
    "envs_str": "highway-v0",
    "envs_params": {
        "lanes_count": [2],
        "initial_spacing": [2],
        "vehicles_count": [2],
        "duration": [3],
        "other_vehicles_type": ["highway_env.vehicle.behavior.IDMVehicle"],
        "centering_position": [[0.3, 0.5]],
        "collision_reward": [0]
    }
  },
  "create_data": {
    "filename_data": "samples.data",
    "as_json": false,
    "handcrafted_greedy_policy": true,
    "trajs_by_ftq_batch": 3,
    "lambda_": 0,
    "normalize_reward": true,
    "N_trajs": 10,
    "epsilon_decay": {
      "start": 1.0,
      "decay": 0.001
    }
  },
  "main": {
    "filename_data": "samples.data",
    "path_data": null,
    "normalize_reward": true,
    "N_trajs": 10
  },
  "bftq_params": {

    "nn_loss_stop_condition": 0.001,
    "max_nn_epoch": 10,
    "optimizer": "ADAM",
    "loss_function": "l2",
    "max_ftq_epoch": 2,
    "learning_rate": 0.001,
    "reset_policy_each_ftq_epoch": true,
    "delta_stop": 0.001,
    "weight_decay": 0.0005,
    "batch_size_experience_replay": "ALL_BATCH",
    "loss_function_c": "l2",
    "weights_losses": [
      1.0,
      1.0
    ]
  },
  "bftq_net_params": {
    "beta_encoder_type": "LINEAR",
    "size_beta_encoder": 5,
    "activation_type": "RELU",
    "reset_type": "XAVIER",
    "intra_layers": [
      32
    ]
  },
  "nb_trajs_between_epoch": 2,
  "lambdas": "[0.]",
  "betas": "np.linspace(0.,1.,4)",
  "betas_test": "np.linspace(0.,1.,4)",
  "ftq_params": {
    "nn_loss_stop_condition": 0.001,
    "max_nn_epoch": 20,
    "optimizer": "ADAM",
    "loss_function": "l2",
    "max_ftq_epoch": 2,
    "learning_rate": 0.001,
    "reset_policy_each_ftq_epoch": true,
    "delta_stop": 0.001,
    "weight_decay": 0.0005,
    "batch_size_experience_replay": "ALL_BATCH",
        "step_test_policy_nn_epoch": 10

  },
  "net_params": {
    "activation_type": "RELU",
    "reset_type": "XAVIER",
    "intra_layers": [
      32
    ]
  },
  "gamma": 1.0,
  "gamma_c": 1.0,
  "dqn_params": {
    "batch_size_experience_replay": 32,
    "target_update": 10,
    "optimizer": "ADAM",
    "loss_function": "l2",
    "lr": 0.001,
    "weight_decay": 0.0005,
    "workspace": "dqn"
  }
}


