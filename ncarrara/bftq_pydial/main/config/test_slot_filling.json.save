    "trajs_by_ftq_batch": 50,
    "reset_weight": false,
    "lambda_": 0,
    "normalize_reward": true,
    "N_trajs": 500,
    "epsilon_decay": {
      "start": 1.0,
      "decay": 0.01
    }
  },
  "main": {
    "path_sample_data": "tmp/test_slot_filling/test_slot_filling.data",
