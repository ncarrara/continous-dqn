      "size_constraints":[3],
      "penalty_if_bye":[0.0],
      "penalty_if_hangup":[0.0],
      "penalty_by_turn":[-5.0],
      "penalty_if_max_turn":[0.0],
      "reward_if_sucess":[100.0]
    }
  },
  "gamma": 1.0,
  "gamma_c": 1.0,
  "create_data": {
