{
  "general": {
    "id": "bandit",
    "seed": 0,
    "workspace": "tmp/bandit",
    "cpu": {
      "processes": null,
      "processes_when_linked_with_gpu": 1
    },
    "is_tensorboardX": false,
    "hull_options": {
      "decimals": null,
      "qhull_options": null,
      "remove_duplicated_points": true,
      "library": "scipy"
    },
    "dictConfig": {
      "version": 1,
      "disable_existing_loggers": false,
      "formatters": {
        "standard": {
          "format": "[%(levelname)s] %(message)s "
        }
      },
      "handlers": {
        "default": {
          "level": "INFO",
          "formatter": "standard",
          "class": "logging.StreamHandler"
        },
        "file": {
          "level": "INFO",
          "formatter": "standard",
          "filename": "bandit.log",
          "class": "logging.FileHandler"
        }
      },
      "loggers": {
        "": {
          "handlers": [
            "default", "file"
          ],
          "level": "INFO",
          "propagate": false
        },
        "ncarrara.budgeted_rl.bftq.pytorch_budgeted_fittedq": {
          "handlers": [
            "default", "file"
          ],
          "level": "DEBUG",
          "propagate": false
        },
        "ncarrara.utils_rl.environments.slot_filling_env.slot_filling_env": {
          "handlers": [
            "default"
          ],
          "level": "WARN",
          "propagate": false
        },
        "ncarrara.utils_rl.environments.envs_factory": {
          "handlers": [
            "default"
          ],
          "level": "WARN",
          "propagate": false
        },
        "ncarrara.utils.os": {
          "handlers": [
            "default"
          ],
          "level": "ERROR",
          "propagate": false
        }
      }
    }
  },
  "feature_str": "feature_identity",
  "generate_envs": {
    "envs_str": "finite-mdp-v0",
    "envs_params": {
      "mode": ["stochastic"],
      "transition": [[[[1], [1]]]],
      "reward": [[[0, 1]]],
      "cost": [[[0, 1]]],
      "terminal": [[false]]
    }
  },
  "gamma": 0.9,
  "gamma_c": 0.9,

  "learn_bftq_egreedy": {
    "trajs_by_ftq_batch": 10,
    "normalize_reward": false,
    "N_trajs": 10,
    "epsilon_decay": {
      "start": 1.0,
      "decay": 0.0
    },
    "betas_for_exploration": "np.linspace(0, 1, 10)"
  },
  "-learn_bftq_full_batch": {
    "load_memory": {
        "path": "../../samples.data",
        "as_json": false
      },
    "normalize_reward": false
  },
  "bftq_params": {
    "use_data_loader": false,
    "use_data_parallel": false,
    "use_extra_gpu_memory_threshold": 100,
    "maximum_number_of_gpu": 2,
    "betas_for_discretisation": "np.linspace(0, 1, 100)",
    "betas_for_duplication": "[]",
    "clamp_Qc": [0, 10],
    "nn_loss_stop_condition": 0.001,
    "max_nn_epoch": 400,
    "optimizer": "ADAM",
    "loss_function": "l2",
    "max_ftq_epoch": 10,
    "learning_rate": 0.01,
    "reset_policy_each_ftq_epoch": true,
    "delta_stop": 0.001,
    "weight_decay": 0,
    "batch_size_experience_replay": "ALL_BATCH",
    "loss_function_c": "l2",
    "weights_losses": [
      1.0,
      1.0
    ]
  },
  "bftq_net_params": {
    "beta_encoder_type": "LINEAR",
    "size_beta_encoder": 50,
    "activation_type": "RELU",
    "reset_type": "XAVIER",
    "intra_layers": [
      128,
      64
    ]
  },
  "test_bftq": {
    "policy_path": "policy.pt",
    "N_trajs": 150,
    "betas_test": "np.linspace(0, 1, 11)"
  },
  "render_bftq": {
    "policy_path": "policy.pt",
    "N_trajs": 5,
    "betas_test": "np.linspace(0.,0.3,3)"
  },

  "-learn_ftq_egreedy": {
    "lambdas": "[0, 1, 2, 10]",
    "trajs_by_ftq_batch": 10,
    "normalize_reward": true,
    "N_trajs": 10,
    "epsilon_decay": {
      "start": 1.0,
      "decay": 0.003
    },
    "save_memory": {
      "path": "learn_ftq_egreedy.data",
      "as_json": false
    }
  },
  "learn_ftq_duplicate": {
    "lambdas": "[0, 1, 2, 10]",
    "generate_data": {
      "lambda_": 0.0,
      "trajs_by_ftq_batch": 10,
      "normalize_reward": true,
      "N_trajs": 10,
      "epsilon_decay": {
        "start": 1.0,
        "decay": 0.0
      },
      "save_memory": {
        "path": "samples_for_duplicate.data",
        "as_json": false
      }
    },
    "learn_ftq_full_batch": {
      "load_memory": {
        "path": "samples_for_duplicate.data",
        "as_json": false
      },
      "normalize_reward": true
    }
  },
  "ftq_params": {
    "nn_loss_stop_condition": 0.0,
    "max_nn_epoch": 400,
    "optimizer": "ADAM",
    "loss_function": "l2",
    "max_ftq_epoch": 10,
    "learning_rate": 0.01,
    "reset_policy_each_ftq_epoch": true,
    "delta_stop": 0.0,
    "weight_decay": 0.0,
    "batch_size_experience_replay": "ALL_BATCH"
  },
  "ftq_net_params": {
    "activation_type": "RELU",
    "reset_type": "XAVIER",
    "intra_layers": [
      64,
      32
    ]
  },
  "-test_ftq": {
    "policy_path": "final_policy.pt",
    "N_trajs": 150
  },
  "test_ftq_duplicate": {
    "N_trajs": 150,
    "policy_path": "policy.pt"
  }
}