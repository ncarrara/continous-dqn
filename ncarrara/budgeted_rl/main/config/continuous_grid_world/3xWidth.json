{
  "general": {
    "id": "3xWidth",
    "seed": 0,
    "cpu": {
      "processes": 1,
      "processes_when_linked_with_gpu": 1
    },
    "workspace": "tmp/3xWidth",
    "dictConfig": {
      "version": 1,
      "disable_existing_loggers": false,
      "formatters": {
        "standard": {
          "format": "[%(levelname)s] %(message)s "
        }
      },
      "handlers": {
        "default": {
          "level": "INFO",
          "formatter": "standard",
          "class": "logging.StreamHandler"
        }
      },
      "loggers": {
        "": {
          "handlers": [
            "default"
          ],
          "level": "DEBUG",
          "propagate": false
        },
        "ncarrara.utils_rl.environments.slot_filling_env.slot_filling_env": {
          "handlers": [
            "default"
          ],
          "level": "WARN",
          "propagate": false
        },
        "ncarrara.utils_rl.environments.envs_factory": {
          "handlers": [
            "default"
          ],
          "level": "WARN",
          "propagate": false
        },
        "ncarrara.utils.os": {
          "handlers": [
            "default"
          ],
          "level": "ERROR",
          "propagate": false
        }
      }
    }
  },
  "generate_envs": {
    "envs_str": "3xWidth",
    "envs_params": {
      "width": [3]
    }
  },
  "feature_str": "feature_identity",
  "gamma": 1.0,
  "gamma_c": 1.0,
  "test_bftq": {
    "policy_path": "final_policy.pt",
    "N_trajs": 100,
    "betas_test": "np.linspace(0.,1.,21)"
  },
  "learn_bftq_egreedy": {
    "trajs_by_ftq_batch": 2000,
    "normalize_reward": true,
    "N_trajs": 2000,
    "epsilon_decay": {
      "start": 1.0,
      "decay": 0.001
    },
    "betas_for_exploration": "np.array([])"
  },
  "bftq_params": {
    "use_data_loader": false,
    "use_data_parallel": false,
    "use_extra_gpu_memory_threshold": 100,
    "maximum_number_of_gpu": 1,
    "betas_for_discretisation": "np.linspace(0.,1.,21)",
    "betas_for_duplication": "np.array([])",
    "nn_loss_stop_condition": 0.001,
    "max_nn_epoch": 5000,
    "optimizer": "ADAM",
    "loss_function": "l2",
    "max_ftq_epoch": 3,
    "learning_rate": 0.001,
    "reset_policy_each_ftq_epoch": true,
    "delta_stop": 0.001,
    "weight_decay": 0.0005,
    "batch_size_experience_replay": "ALL_BATCH",
    "loss_function_c": "l2",
    "weights_losses": [
      1.0,
      1.0
    ]
  },
  "bftq_net_params": {
    "beta_encoder_type": "LINEAR",
    "size_beta_encoder": 50,
    "activation_type": "RELU",
    "reset_type": "XAVIER",
    "intra_layers": [
      256,
      128,
      64
    ]
  }
}


