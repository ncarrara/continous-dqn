{
  "general": {
    "id": "double_path_main",
    "seed": 0,
    "workspace": "tmp/double_path_main",
    "hull_options": {
      "decimals": null,
      "qhull_options": null,
      "remove_duplicated_points": true,
      "library": "pure_python"
    },
    "gpu": {
      "split_batches": 5
    },
    "cpu": {
      "processes": null,
      "processes_when_linked_with_gpu": 5
    },
    "dictConfig": {
      "version": 1,
      "disable_existing_loggers": false,
      "formatters": {
        "standard": {
          "format": "[%(levelname)s] %(message)s "
        }
      },
      "handlers": {
        "default": {
          "level": "ERROR",
          "formatter": "standard",
          "class": "logging.StreamHandler"
        }
      },
      "loggers": {
        "": {
          "handlers": [
            "default"
          ],
          "level": "ERROR",
          "propagate": false
        },
        "ncarrara.utils_rl.environments.slot_filling_env.slot_filling_env": {
          "handlers": [
            "default"
          ],
          "level": "ERROR",
          "propagate": false
        },
        "ncarrara.utils_rl.environments.envs_factory": {
          "handlers": [
            "default"
          ],
          "level": "ERROR",
          "propagate": false
        },
        "ncarrara.utils.os": {
          "handlers": [
            "default"
          ],
          "level": "ERROR",
          "propagate": false
        }
      }
    }
  },
  "feature_str": "feature_identity",
  "generate_envs": {
    "envs_str": "double_path",
    "envs_params": {
      "high": [
        9
      ],
      "std": [
        [
          0.25,
          0.25
        ]
      ]
    }
  },

  "test_ftq_duplicate": {
    "N_trajs": 500,
    "policy_path": "policy.pt"
  },
  "test_bftq": {
    "policy_path": "final_policy.pt",
    "N_trajs": 500,
    "betas_test": "np.linspace(0.,10.,1)"
  },

  "gamma": 1.0,
  "gamma_c": 1.0,

  "learn_ftq_duplicate": {
    "lambdas": "[0.,5.,10.,50.,100.,500.,1000.]",
    "generate_data": {
      "lambda_": 0.0,
      "trajs_by_ftq_batch": 100,
      "normalize_reward": true,
      "N_trajs": 1000,
      "epsilon_decay": {
        "start": 1.0,
        "decay": 0.005
      },
      "save_memory": {
        "path": "samples_for_duplicate.data",
        "as_json": false
      }
    },
    "learn_ftq_full_batch": {
      "load_memory": {
        "path": "samples_for_duplicate.data",
        "as_json": false
      },
      "normalize_reward": true
    }
  },
  "learn_bftq_egreedy": {
    "trajs_by_ftq_batch": 100,
    "normalize_reward": true,
    "N_trajs": 1000,
    "epsilon_decay": {
      "start": 1.0,
      "decay": 0.005
    },
    "betas_for_exploration": "[]",
    "betas_for_duplication": "[]"
  },

  "bftq_params": {
    "clamp_Qc": null,
    "use_data_loader": false,
    "use_data_parallel": false,
    "use_extra_gpu_memory_threshold": 100,
    "maximum_number_of_gpu": 1,
    "betas_for_discretisation": "np.linspace(0.,1.,101)",
    "nn_loss_stop_condition": 0.001,
    "max_nn_epoch": 5000,
    "optimizer": "ADAM",
    "loss_function": "l2",
    "max_ftq_epoch": 12,
    "learning_rate": 0.001,
    "reset_policy_each_ftq_epoch": true,
    "delta_stop": 0.001,
    "weight_decay": 0.0005,
    "batch_size_experience_replay": "ALL_BATCH",
    "loss_function_c": "l2",
    "weights_losses": [
      1.0,
      1.0
    ]
  },
  "bftq_net_params": {
    "beta_encoder_type": "LINEAR",
    "size_beta_encoder": 2,
    "activation_type": "RELU",
    "reset_type": "XAVIER",
    "intra_layers": [
      256,
      128,
      64
    ]
  },
  "ftq_params": {
    "nn_loss_stop_condition": 0.001,
    "max_nn_epoch": 5000,
    "optimizer": "ADAM",
    "loss_function": "l2",
    "max_ftq_epoch": 12,
    "learning_rate": 0.001,
    "reset_policy_each_ftq_epoch": true,
    "delta_stop": 0.001,
    "weight_decay": 0.0005,
    "batch_size_experience_replay": "ALL_BATCH"
  },
  "ftq_net_params": {
    "activation_type": "RELU",
    "reset_type": "XAVIER",
    "intra_layers": [
      128,
      64,
      32
    ]
  }
}


