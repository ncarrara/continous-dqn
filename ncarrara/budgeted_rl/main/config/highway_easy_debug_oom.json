{
  "general": {
    "id": "highway_easy_debug_oom",
    "seed": 0,
    "workspace": "tmp/highway_easy_debug_oom",
    "dictConfig": {
      "version": 1,
      "disable_existing_loggers": false,
      "formatters": {
        "standard": {
          "format": "[%(levelname)s] %(message)s "
        }
      },
      "handlers": {
        "default": {
          "level": "INFO",
          "formatter": "standard",
          "class": "logging.StreamHandler"
        },
        "file": {
          "level": "INFO",
          "formatter": "standard",
          "filename": "highway_easy.log",
          "class": "logging.FileHandler"
        }
      },
      "loggers": {
        "": {
          "handlers": [
            "default", "file"
          ],
          "level": "INFO",
          "propagate": false
        },
        "ncarrara.utils_rl.environments.slot_filling_env.slot_filling_env": {
          "handlers": [
            "default"
          ],
          "level": "WARN",
          "propagate": false
        },
        "ncarrara.utils_rl.environments.envs_factory": {
          "handlers": [
            "default"
          ],
          "level": "INFO",
          "propagate": false
        },
        "ncarrara.utils.os": {
          "handlers": [
            "default"
          ],
          "level": "ERROR",
          "propagate": false
        }
      }
    }
  },
  "feature_str": "feature_identity",
  "generate_envs": {
    "envs_str": "highway-v0",
    "envs_params": {
        "lanes_count": [4],
        "initial_spacing": [3],
        "vehicles_count": [50],
        "duration": [40],
        "other_vehicles_type": ["highway_env.vehicle.behavior.IDMVehicle"],
        "centering_position": [[0.3, 0.5]],
        "collision_reward": [0]
    }
  },
  "gamma": 0.8,
  "gamma_c": 1.0,
  "lambdas": "[]",

  "learn_bftq_egreedy": {
    "trajs_by_ftq_batch": 1,
    "normalize_reward": false,
    "N_trajs": 10,
    "epsilon_decay": {
      "start": 1.0,
      "decay": 0.002
    },
    "betas_for_exploration": "[1.]"
  },
  "bftq_params": {
    "betas_for_discretisation": "np.linspace(0.,1.,11)",
    "betas_for_duplication": "np.linspace(0.,1.,1e4)",
    "nn_loss_stop_condition": 0.0,
    "max_nn_epoch": 10,
    "optimizer": "ADAM",
    "loss_function": "l2",
    "max_ftq_epoch": 2,
    "learning_rate": 0.01,
    "reset_policy_each_ftq_epoch": true,
    "delta_stop": 0.0,
    "weight_decay": 0.0,
    "batch_size_experience_replay": "ALL_BATCH",
    "loss_function_c": "l2",
    "weights_losses": [
      1.0,
      1.0
    ]
  },
  "bftq_net_params": {
    "beta_encoder_type": "LINEAR",
    "size_beta_encoder": 50,
    "activation_type": "RELU",
    "reset_type": "XAVIER",
    "intra_layers": [
      256,
      128,
      64
    ]
  },
  "ftq_params": {
    "nn_loss_stop_condition": 0.0,
    "max_nn_epoch": 400,
    "optimizer": "ADAM",
    "loss_function": "l2",
    "max_ftq_epoch": 15,
    "learning_rate": 0.01,
    "reset_policy_each_ftq_epoch": true,
    "delta_stop": 0.0,
    "weight_decay": 0.0,
    "batch_size_experience_replay": "ALL_BATCH"
  },
  "ftq_net_params": {
    "activation_type": "RELU",
    "reset_type": "XAVIER",
    "intra_layers": [
      128,
      64,
      32
    ]
  }
}